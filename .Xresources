! URXVT
URxvt*.scrollBar: 	false
URxvt*.depth: 		32
URxvt*font:             xft:Sauce Code Powerline:size=9, xft:Menlo for Powerline:size=9

#define base00 #090F10
#define base01 #c5f779
#define base02 #ffff66
#define base03 #505050
#define base04 #3377ff
#define base05 #d0d0d0
#define base06 #ff4433
#define base07 #f5f5f5
#define base08 #ac4142
#define base09 #d28445
#define base0A #f4bf75
#define base0B #90a959
#define base0C #75b5aa
#define base0D #6a9fb5
#define base0E #aa759f
#define base0F #8f5536

*.foreground: base05
URxvt*.background: [90]base00
*.background: base00
*.cursorColor: base05
*.color0: #333343
*.color1: base08
*.color2: base0B
*.color3: base0A
*.color4: base0D
*.color5: base0E
*.color6: base0C
*.color7: base05
*.color8: base03
*.color9: base09
*.color10: base01
*.color11: base02
*.color12: base04
*.color13: base06
*.color14: base0F
*.color15: base07

! these three lines enable clicking on links to open them ;)
! if you want to enable tab support, append ,tabbed to the next line
! then use shift+down arrow to create tabs and shift+{left,right} arrows to switch between them
URxvt*perl-ext-common: default,matcher
URxvt*matcher.button: 1
URxvt*url-launcher: xdg-open
